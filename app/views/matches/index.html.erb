      
      <div class = "col-xs-9"

<h2>Matches </h2>
<div class = "panel panel-default" >
<table class = "table table-striped">
  <thead>
    <tr>
      <th>Host : Guest</th>
      <th> </th>
      <th>Game date </th>
      <th>Votes</th>
      <th>Rating</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @matches.each do |match| %>
      <tr>
        <td><%= match.host %> : <%= match.guest %></td>
        <td><%= match.host_score %> :<%= match.guest_score %> </td>
        <td><%= match.day %></td>
        <td><%= match.voted.to_i %></td>
        <td><%= match.rating.to_f.round(2) %></td>
       
        <td><%= link_to 'Show', match %></td>
          <% if user_signed_in? %>

        <td><%= link_to 'Edit', edit_match_path(match) %></td>
        <td><%= link_to 'Destroy', match, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <%end%>
      </tr>
    <% end %>
  </tbody>
</table>


</div>
</div>
<br>


    <div class=" col-xs-3 panel panel-default">
      <div class = "panel-heading"> Recently added matches </div>
      <div class = "panel-body">
        <%=render 'recent'%>
        </div>
      </div>

